{
    "data_dir": "/raid/krb/oxford-radar-robotcar-dataset/",
    "log_dir": "./runs/date",
    "gpuid": "cuda:0",
    "min_range": 58,
    "radar_resolution": 0.0432,
    "cart_resolution": 0.3456,
    "cart_pixel_width": 512,
    "remove_motion": false,
    "remove_doppler": false,
    "train_split": [0, 22],
    "validation_split": [22, 25],
    "test_split": [25, 32],
    "dataset": {"images": {"input_channel": ["intensity"]}, "sensor": "radar"},
    "networks": {
        "unet": {
            "bilinear": true,
            "first_feature_dimension": 8,
        },
        "keypoint_block": {
            "patch_size": 16,
            "softmax_temp": 1.0,
        },
        "matcher_block": {
            "softmax_temp": 0.01
        },
        "outlier_rejection": {
            "on": true,
            "threshold": 4.0
        }
    },
    "lr": 0.0001,
    "weight_decay": 0,
    "clip_norm": 1.0,
    "loss": {
        "types": ["keypoint_2D", "svd"],
        "weights": {"keypoint_2D":  0.01, "svd": 10.0},
    },
    "batch_size": 2,
    "window_size": 2,
    "num_workers": 2,
    "max_iterations": 200000,
    "print_rate": 10,
    "log_rate": 10,
    "vis_rate": 1000,
    "save_rate": 5000,
    "val_rate": 5000,
    "vis_num": 4,
}
