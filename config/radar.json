{
    "data_dir": "/raid/krb/oxford-radar-robotcar-dataset/",
    "log_dir": "./runs/date",
    "gpuid": "cuda:0",
    "min_range": 58,
    "radar_resolution": 0.0432,
    "cart_resolution": 0.3456,
    "cart_pixel_width": 512,
    "remove_motion": false,
    "remove_doppler": false,
    "train_split": [0, 22],
    "validation_split": [22, 25],
    "test_split": [25, 32],
    "dataset": {"images": {"input_channel": ["intensity"]}, "sensor": "radar"},
    "networks": {
        "unet": {
            "bilinear": true,
            "first_feature_dimension": 8,
        },
        "keypoint_block": {
            "patch_height": 16,
            "patch_width": 16,
            "softmax_temp": 1.0,
            "grid_sample": true
        },
        "matcher_block": {
            "softmax_temperature": 0.01
        },
        "match_type": "zncc",
        "dense_match": true,
        "outlier_rejection": {
            "on": true,
            "type": "3D",
            "threshold": 4.0
        }
    },
    "lr": 0.0001,
    "weight_decay": 0,
    "clip_norm": 1.0,
    "loss": {
        "types": ["keypoint_3D", "svd"],
        "weights": {"keypoint_2D":  0.01, "keypoint_3D":  1.0, "svd": 10.0},
        "start_svd_epoch": 2
    },
    "save_names": ["T_tgt_src", "T_tgt_src_pred", "inliers", "src_valid", "pseudo_valid", "weights"],
    "batch_size": 2,
    "window_size": 2,
    "num_workers": 2,
    "max_epochs": 100,

    "print_rate": 10,
    "log_rate": 10,
    "vis_rate": 1000,
    "save_rate": 5000,
    "eval_rate": 5000,
    "vis_num": 4,
}
