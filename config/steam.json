        "mask_detector_scores": true,
        "zero_vel_prior": true,
        "weight_matrix": true
    },
    "lr": 0.00001,
    "weight_decay": 0,
    "clip_norm": 1.0,
    "batch_size": 1,
    "window_size": 4,
    "num_workers": 4,
    "max_iterations": 300000,
    "max_epochs": 3,
    "print_rate": 10,
    "log_rate": 10,
    "save_rate": 5000,
    "val_rate": 5000,
    "vis_num": 6,
    "vis_keypoint_nms": 0,
